# Environment variables for comprehensive test configuration

# Slack webhook URLs for notifications
# GSCR_SLACK_WEBHOOK_URL_SUCCESS: Webhook for success notifications (INFO level)
# GSCR_SLACK_WEBHOOK_URL_ERROR: Webhook for error notifications (WARN/ERROR levels)
# Note: If SUCCESS is set, ERROR must also be set
GSCR_SLACK_WEBHOOK_URL_SUCCESS=
GSCR_SLACK_WEBHOOK_URL_ERROR=

# Test environment variables for comprehensive testing
TEST_GLOBAL_VAR=global_test_value
COMPREHENSIVE_TEST=enabled
NODE_ENV=test
DEBUG_MODE=true
FINAL_TEST_VAR=comprehensive_success
TEST_SECURITY_VAR=security_value
